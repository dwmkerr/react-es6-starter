    "babel": "5.x.x",
    "babel-loader": "5.x.x",
    "webpack": "^1.12.1",
    "http-server": "0.x.x"
  },
  "devDependencies": {
    "babel-core": "5.x.x",
    "html-webpack-plugin": "^1.6.1",
    "isparta": "^3.0.4",
    "isparta-instrumenter-loader": "^0.2.1",
    "istanbul": "^0.3.19",
    "jasmine": "2.x.x",
    "jasmine-core": "2.x.x",
    "jquery": "^2.1.4",
    "karma": "0.x.x",
    "karma-chrome-launcher": "^0.2.0",
